PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX xlink: <http://www.w3.org/1999/xlink/>
PREFIX ag: <http://www.ldc.upenn.edu/atlas/ag/>
PREFIX : <http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER#>

<http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER> ag:agset :treebank1.
:treebank1 a ag:AGSet;
  ag:version "1.0";
  dc:title "An example of treebank annotation".

:treebank1 ag:timeline :T1.
:T1 a ag:Timeline.

:T1 ag:signal :S1.
:S1 a ag:Signal;
  ag:mimeClass "text";
  ag:mimeType "simpletext";
  ag:encoding "utf8";
  ag:unit "byte";
  xlink:href "textfile.utf8";
  xlink:type "simple".

:treebank1 ag:ag :t1.
:t1 a ag:AG;
  ag:timeline :T1;
  ag:type "treebank".

:t1 ag:anchor :A0.
:A0 a ag:Anchor;
  ag:offset "0";
  ag:unit "byte".

:t1 ag:anchor :A1.
:A1 a ag:Anchor;
  ag:offset "3";
  ag:unit "byte".

:t1 ag:anchor :A2.
:A2 a ag:Anchor;
  ag:offset "4";
  ag:unit "byte".

:t1 ag:anchor :A3.
:A3 a ag:Anchor;
  ag:offset "9";
  ag:unit "byte".

:t1 ag:anchor :A4.
:A4 a ag:Anchor;
  ag:offset "10";
  ag:unit "byte".

:t1 ag:anchor :A5.
:A5 a ag:Anchor;
  ag:offset "13";
  ag:unit "byte".

:t1 ag:annotation :Ann0.
:Ann0 a ag:Annotation;
  ag:start :A0;
  ag:end :A1;
  ag:type "syn".

:Ann0 ag:pos-tag "DT".

:t1 ag:annotation :Ann1.
:Ann1 a ag:Annotation;
  ag:start :A2;
  ag:end :A3;
  ag:type "syn".

:Ann1 ag:pos-tag "JJ".

:t1 ag:annotation :Ann2.
:Ann2 a ag:Annotation;
  ag:start :A4;
  ag:end :A5;
  ag:type "syn".

:Ann2 ag:pos-tag "NN".

:t1 ag:annotation :Ann3.
:Ann3 a ag:Annotation;
  ag:start :A0;
  ag:end :A5;
  ag:type "syn".

:Ann3 ag:pos-tag "NP".

