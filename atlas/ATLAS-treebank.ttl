PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX xlink: <http://www.w3.org/1999/xlink/>
PREFIX atlas: <http://www.ldc.upenn.edu/atlas/ag/>
PREFIX : <http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER#>

<http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER> atlas:agset :treebank1.
:treebank1 a atlas:AGSet;
  atlas:version "1.0";
  dc:title "An example of treebank annotation".

:treebank1 atlas:timeline :T1.
:T1 a atlas:Timeline.

:T1 atlas:signal :S1.
:S1 a atlas:Signal;
  atlas:mimeClass "text";
  atlas:mimeType "simpletext";
  atlas:encoding "utf8";
  atlas:unit "byte";
  xlink:href "textfile.utf8";
  xlink:type "simple".

:treebank1 atlas:ag :t1.
:t1 a atlas:AG;
  atlas:timeline :T1;
  atlas:type "treebank".

:t1 atlas:anchor :A0.
:A0 a atlas:Anchor;
  atlas:offset "0";
  atlas:unit "byte".

:t1 atlas:anchor :A1.
:A1 a atlas:Anchor;
  atlas:offset "3";
  atlas:unit "byte".

:t1 atlas:anchor :A2.
:A2 a atlas:Anchor;
  atlas:offset "4";
  atlas:unit "byte".

:t1 atlas:anchor :A3.
:A3 a atlas:Anchor;
  atlas:offset "9";
  atlas:unit "byte".

:t1 atlas:anchor :A4.
:A4 a atlas:Anchor;
  atlas:offset "10";
  atlas:unit "byte".

:t1 atlas:anchor :A5.
:A5 a atlas:Anchor;
  atlas:offset "13";
  atlas:unit "byte".

:t1 atlas:annotation :Ann0.
:Ann0 a atlas:Annotation;
  atlas:start :A0;
  atlas:end :A1;
  atlas:type "syn";
  atlas:start "A0";
  atlas:end "A1".

:Ann0 atlas:pos-tag "DT".

:t1 atlas:annotation :Ann1.
:Ann1 a atlas:Annotation;
  atlas:start :A2;
  atlas:end :A3;
  atlas:type "syn";
  atlas:start "A2";
  atlas:end "A3".

:Ann1 atlas:pos-tag "JJ".

:t1 atlas:annotation :Ann2.
:Ann2 a atlas:Annotation;
  atlas:start :A4;
  atlas:end :A5;
  atlas:type "syn";
  atlas:start "A4";
  atlas:end "A5".

:Ann2 atlas:pos-tag "NN".

:t1 atlas:annotation :Ann3.
:Ann3 a atlas:Annotation;
  atlas:start :A0;
  atlas:end :A5;
  atlas:type "syn";
  atlas:start "A0";
  atlas:end "A5".

:Ann3 atlas:pos-tag "NP".

