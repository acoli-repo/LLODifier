PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX xlink: <http://www.w3.org/1999/xlink/>
PREFIX ag: <http://www.ldc.upenn.edu/atlas/ag/>
PREFIX : <http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER#>

<http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER> ag:agset :AL1.
:AL1 a ag:AGSet;
  ag:version "1.0";
  dc:title "Alignment of annotations of two signals".

:AL1 ag:timeline :T1.
:T1 a ag:Timeline.

:T1 ag:signal :S1.
:S1 a ag:Signal;
  ag:mimeClass "audio";
  ag:mimeType "wav";
  ag:encoding "wav";
  ag:unit "16kHz";
  xlink:href "japanese.wav";
  xlink:type "simple".

:T1 ag:signal :S2.
:S2 a ag:Signal;
  ag:mimeClass "audio";
  ag:mimeType "wav";
  ag:encoding "wav";
  ag:unit "16kHz";
  xlink:href "english.wav";
  xlink:type "simple".

:AL1 ag:ag :t1.
:t1 a ag:AG;
  ag:timeline :T1;
  ag:type "alignment".

:t1 ag:anchor :A0.
:A0 a ag:Anchor;
  ag:signals "S1";
  ag:offset "10000";
  ag:unit "16kHz".

:t1 ag:anchor :A1.
:A1 a ag:Anchor;
  ag:signals "S1";
  ag:offset "26000";
  ag:unit "16kHz".

:t1 ag:anchor :A2.
:A2 a ag:Anchor;
  ag:signals "S2";
  ag:offset "30000";
  ag:unit "16kHz".

:t1 ag:anchor :A3.
:A3 a ag:Anchor;
  ag:signals "S2";
  ag:offset "46000";
  ag:unit "16kHz".

:t1 ag:annotation :Ann0.
:Ann0 a ag:Annotation;
  ag:start :A0;
  ag:end :A1;
  ag:type "transcription".

:Ann0 ag:trans "Kore-wa neko-desu".

:t1 ag:annotation :Ann1.
:Ann1 a ag:Annotation;
  ag:start :A2;
  ag:end :A3;
  ag:type "transcription".

:Ann1 ag:trans "This is a cat.".

:Ann1 ag:align :Ann0.

