PREFIX dc: <http://purl.org/dc/elements/1.1/>
PREFIX xlink: <http://www.w3.org/1999/xlink/>
PREFIX atlas: <http://www.ldc.upenn.edu/atlas/ag/>
PREFIX : <http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER#>

<http://example.org/PLEASE_PROVIDE_baseURI_PARAMETER> atlas:agset :AL1.
:AL1 a atlas:AGSet;
  atlas:version "1.0";
  dc:title "Alignment of annotations of two signals".

:AL1 atlas:timeline :T1.
:T1 a atlas:Timeline.

:T1 atlas:signal :S1.
:S1 a atlas:Signal;
  atlas:mimeClass "audio";
  atlas:mimeType "wav";
  atlas:encoding "wav";
  atlas:unit "16kHz";
  xlink:href "japanese.wav";
  xlink:type "simple".

:T1 atlas:signal :S2.
:S2 a atlas:Signal;
  atlas:mimeClass "audio";
  atlas:mimeType "wav";
  atlas:encoding "wav";
  atlas:unit "16kHz";
  xlink:href "english.wav";
  xlink:type "simple".

:AL1 atlas:ag :t1.
:t1 a atlas:AG;
  atlas:timeline :T1;
  atlas:type "alignment".

:t1 atlas:anchor :A0.
:A0 a atlas:Anchor;
  atlas:signals "S1";
  atlas:offset "10000";
  atlas:unit "16kHz".

:t1 atlas:anchor :A1.
:A1 a atlas:Anchor;
  atlas:signals "S1";
  atlas:offset "26000";
  atlas:unit "16kHz".

:t1 atlas:anchor :A2.
:A2 a atlas:Anchor;
  atlas:signals "S2";
  atlas:offset "30000";
  atlas:unit "16kHz".

:t1 atlas:anchor :A3.
:A3 a atlas:Anchor;
  atlas:signals "S2";
  atlas:offset "46000";
  atlas:unit "16kHz".

:t1 atlas:annotation :Ann0.
:Ann0 a atlas:Annotation;
  atlas:start :A0;
  atlas:end :A1;
  atlas:type "transcription";
  atlas:start "A0";
  atlas:end "A1".

:Ann0 atlas:trans "Kore-wa neko-desu".

:t1 atlas:annotation :Ann1.
:Ann1 a atlas:Annotation;
  atlas:start :A2;
  atlas:end :A3;
  atlas:type "transcription";
  atlas:start "A2";
  atlas:end "A3".

:Ann1 atlas:trans "This is a cat.".

:Ann1 atlas:align :Ann0.

